      "destination": "/api/response-builder.html"
    },
    {
      "source": "/api/socket-events",
      "destination": "/api/socket-events.html"
    },
    {
      "source": "/api/socket-rooms",
      "destination": "/api/socket-rooms.html"
    },
    {
      "source": "/api/socket-middleware",
      "destination": "/api/socket-middleware.html"
    },
    {
      "source": "/api/socket",
      "destination": "/api/socket.html"
