{
  "buildCommand": "npm run docs:build",
  "outputDirectory": ".vitepress/dist",
  "installCommand": "npm ci",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/express-errors",
      "destination": "/api/express-errors.html"
    },
    {
      "source": "/api/express-request",
