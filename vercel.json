    },
    {
      "source": "/guide/migration",
      "destination": "/guide/migration.html"
    },
    {
      "source": "/guide/basic",
      "destination": "/guide/basic.html"
    },
    {
      "source": "/guide/advanced",
      "destination": "/guide/advanced.html"
    },
    {
      "source": "/guide/environment",
      "destination": "/guide/environment.html"
    },
    {
      "source": "/guide/performance",
      "destination": "/guide/performance.html"
